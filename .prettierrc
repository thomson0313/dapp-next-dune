{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "jsxSingleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "insertPragma": false,
  "requirePragma": false,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.ts?(x)",
      "options": {
        "parser": "typescript"
      }
    }
  ],
  "plugins": ["prettier-plugin-tailwindcss"]
}
